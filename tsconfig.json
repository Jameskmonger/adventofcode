{
  "compileOnSave": true,

  "compilerOptions": {
    "target": "ES2017",
    "sourceMap": true,
    "removeComments": true,
    "lib": [
      "dom",
      "ES2019"
    ],
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noResolve": false,
    "moduleResolution": "node"
  }
}
